{
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@components": "./src/components",
          "@content": "./content",
          "@layouts": "./src/layouts",
          "@hooks": "./src/hooks",
          "@utils": "./src/utils",
          "@theme": "./src/theme",
          "@lib": "./src/lib"
        }
      }
    ]
  ]
}
