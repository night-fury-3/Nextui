{
  "name": "nextui",
  "version": "0.1.1",
  "license": "MIT",
  "description": "ðŸš€ Beautiful and modern React UI library.",
  "homepage": "https://nextui.org",
  "bugs": {
    "url": "https://github.com/jrgarciadev/next-ui/issues/new/choose"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jrgarciadev/next-ui"
  },
  "keywords": [
    "next",
    "next ui",
    "components",
    "modern components",
    "react components",
    "react ui"
  ],
  "main": "dist/index.js",
  "exports": "./dist/nextui.modern.js",
  "module": "dist/index.module.js",
  "types": "dist/index.d.ts",
  "unpkg": "dist/index.umd.js",
  "source": "./src/index.ts",
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "clear": "rm -rf ./dist ./esm",
    "build:prod": "yarn clear && yarn microbundle --compress --strict --format modern,cjs",
    "build:watch": "yarn clear && microbundle watch --no-compress --format modern,cjs",
    "build": "yarn build:prod",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --color",
    "format": "prettier --write 'src/**/*.{ts,tsx,scss,css,json}'",
    "prettier": "prettier --write .",
    "test": "jest --config .jest.config.js --no-cache",
    "test-update": "jest --config .jest.config.js --no-cache --update-snapshot",
    "coverage": "yarn test --coverage",
    "release": "yarn build && yarn publish --access public --non-interactive",
    "storybook": "start-storybook -p 6006 --no-manager-cache",
    "build-storybook": "build-storybook"
  },
  "files": [
    "/dist",
    "/esm"
  ],
  "dependencies": {
    "styled-jsx": "^3.4.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.13.17",
    "@mapbox/rehype-prism": "^0.6.0",
    "@mdx-js/loader": "^1.6.22",
    "@next/mdx": "^10.1.3",
    "@storybook/addon-a11y": "^6.2.8",
    "@storybook/addon-actions": "^6.2.8",
    "@storybook/addon-docs": "^6.2.8",
    "@storybook/addon-essentials": "^6.2.8",
    "@storybook/addon-knobs": "^6.2.8",
    "@storybook/addon-links": "^6.2.8",
    "@storybook/addon-storysource": "^6.2.8",
    "@storybook/react": "^6.2.8",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/styled-jsx": "^2.2.8",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.24.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-config-ts-lambdas": "^1.2.3",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "^5.7.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fs-extra": "^9.1.0",
    "microbundle": "^0.13.0",
    "next": "^10.1.3",
    "path": "^0.12.7",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "prettier-eslint-cli": "^5.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "storybook-addon-react-docgen": "^1.2.42",
    "typescript": "^4.2.4",
    "webpack": "^5.35.0",
    "webpack-cli": "^4.6.0"
  }
}
