    "webpack-merge": "^5.8.0"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "17.0.33"
    }
  },
  "engines": {
    "node": ">=16.x",
    "pnpm": ">=7.x"
  },
  "packageManager": "pnpm@7.14.2"
}
