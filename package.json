    "jest-watch-typeahead": "1.1.0",
    "@swc-node/jest": "^1.5.2",
    "@swc/core": "^1.2.177",
    "@testing-library/jest-dom": "^5.16.4",
    "chalk": "^4.1.2",
    "eslint": "^7.29.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-config-react-app": "^6.0.0",
