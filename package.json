    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "jest-watch-typeahead": "1.1.0",
    "lint-staged": "^13.0.3",
    "markdown-toc": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "p-iteration": "^1.1.8",
    "parcel": "^2.3.1",
    "plop": "3.1.1",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "prettier-eslint-cli": "^5.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "tsup": "6.4.0",
    "turbo": "1.6.3",
    "typescript": "4.6.2",
    "uuid": "^8.3.2",
    "webpack": "^5.53.0",
    "webpack-bundle-analyzer": "^4.4.2",
