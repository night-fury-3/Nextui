{
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "dist/**", "lib/**"]
    },
    "build:fast": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build:fast"]
    },
    "typecheck": {
      "cache": false,
      "dependsOn": ["^typecheck"]
    },
    "start": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "sb": {
      "cache": false
    }
  },
  "globalDependencies": ["tsconfig.json"]
}
