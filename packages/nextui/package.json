{
  "name": "@nextui-org/react",
  "version": "1.0.1-alpha.31",
  "license": "MIT",
  "description": "ðŸš€ Beautiful and modern React UI library.",
  "homepage": "https://nextui.org",
  "bugs": {
    "url": "https://github.com/nextui-org/nextui/issues/new/choose"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nextui-org/nextui"
  },
  "keywords": [
    "next",
    "next ui",
    "components",
    "modern components",
    "react components",
    "react ui"
  ],
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "module": "esm/index.js",
  "unpkg": "dist/index.min.js",
  "main": "dist/index.js",
  "sideEffects": false,
  "scripts": {
    "clear": "rm -rf ./dist ./esm",
    "clear:packages": "rm -rf ./dist/packages",
    "build:types": "tsc -p ./buildconfig",
    "build:rollup": "rollup -c",
    "build:webpack": "webpack --config ./buildconfig/webpack.prod.js",
    "build:watch": "webpack ---config ./buildconfig/webpack.dev.js",
    "build:microbundle": "microbundle build --jsx React.createElement --jsxFragment React.Fragment --tsconfig tsconfig.build.json --strict --globals styled-jsx/style=_JSXStyle,styled-jsx/server=flush",
    "build:microbundle-watch": "microbundle watch --jsx React.createElement --jsxFragment React.Fragment --no-compress --format modern,cjs",
    "build": "yarn clear && yarn build:webpack && yarn build:types && yarn clear:packages",
    "watch": "yarn clear && yarn build:types && yarn build:watch",
    "storybook": "start-storybook -p 6006 --no-manager-cache",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "build-storybook": "build-storybook",
    "start-storybook": "yarn build-storybook && http-server ./storybook-static",
    "test": "NODE_ENV=test jest --config jest.config.js --no-cache",
    "test-update": "NODE_ENV=test jest --config jest.config.js --no-cache --update-snapshot",
    "coverage": "yarn test --coverage",
    "coverage-update": "yarn test-update --coverage",
    "coverage:codecov": "yarn test --coverage && codecov"
  },
  "files": [
    "/dist",
    "/esm"
  ],
  "dependencies": {
    "styled-jsx": "^3.4.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/runtime": "^7.9.6",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@storybook/addon-a11y": "^6.3.7",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/addon-storysource": "^6.3.7",
    "@storybook/react": "^6.3.7",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.22",
    "@types/node": "^15.12.4",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/styled-jsx": "^2.2.8",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "css-mediaquery": "^0.1.2",
    "enzyme": "^3.11.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-react": "^7.24.0",
    "jest": "^26.6.3",
    "microbundle": "^0.13.0",
    "prettier": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.52.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-local-resolve": "^1.0.7",
    "storybook": "^6.3.7",
    "ts-jest": "^26.5.5",
    "typescript": "^4.3.4",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^5.8.0"
  }
}
