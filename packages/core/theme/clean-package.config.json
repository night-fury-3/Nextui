        "require": "./dist/plugin.cjs.js"
      },
      "./colors": {
        "types": "./dist/colors.d.ts",
        "import": "./dist/colors.esm.js",
        "require": "./dist/colors.cjs.js"
      },
      "./package.json": "./package.json"
    }
  }
}
