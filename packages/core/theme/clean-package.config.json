    "types": "dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.esm.js",
        "require": "./dist/index.cjs.js"
      },
      "./plugin": {
        "types": "./dist/plugin.d.ts",
